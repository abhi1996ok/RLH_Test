    <div class="mb-3" style="padding-left: 15px; padding-right: 15px">
        <h6>Duration</h6>
        <form class="">
            <div class="custom-control custom-checkbox ">
                <input type="checkbox" class="custom-control-input" id="customCheck1" name="custom1">
                <label class="custom-control-label" for="customCheck">1-3 Days</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customCheck" name="custom2">
                <label class="custom-control-label" for="customCheck">4-6 Days</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customCheck" name="custom3">
                <label class="custom-control-label" for="customCheck">7-11 Days</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customCheck" name="custom4">
                <label class="custom-control-label" for="customCheck">12 or More Days</label>
            </div>
        </form>
    </div>


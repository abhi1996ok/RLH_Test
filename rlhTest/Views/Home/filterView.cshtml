@model rlhTest.Models.HelperModel.Filters


    <div class="mt-3" style="padding-left: 15px; padding-right: 15px">
        <h6>Type Of Destination</h6>
        <form class="">
            <div class="custom-control custom-checkbox ">
                <input type="checkbox" class="custom-control-input" id="countryCombo" name="combo">
                <label class="custom-control-label" for="customCheck">Country Combo</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="singleCountry" name="single">
                <label class="custom-control-label" for="customCheck">Single Country</label>
            </div>
        </form>
    </div>


    <hr style="border-top: 0.1rem solid #DEE2E6; ">



    <div style="padding-left: 15px; padding-right: 15px">
        <h6>Budget</h6>
        <form class="">
            <div class="custom-control custom-checkbox ">
                <input type="checkbox" class="custom-control-input" id="economy" name="economy">
                <label class="custom-control-label" for="customCheck">Economy</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="standard" name="standard">
                <label class="custom-control-label" for="customCheck">Standard</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="luxury" name="luxury">
                <label class="custom-control-label" for="customCheck">Luxury</label>
            </div>
        </form>
    </div>
    <hr style="border-top: 0.1rem solid #DEE2E6; ">

    <div style="padding-left: 15px; padding-right: 15px">
        <h6>Price (INR)</h6>
        <form action="">

            <!--<input type="range" class="custom-range" id="customRange" name="points1" min="50000"max="500000">-->

            <div class="custom-control custom-checkbox ">
                <input type="checkbox" class="custom-control-input" id="customprice" name="price1">
                <label class="custom-control-label" for="customCheck">Upto 50000</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customprice1" name="price2">
                <label class="custom-control-label" for="customCheck">50000-100000</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customprice2" name="price3">
                <label class="custom-control-label" for="customCheck">100000-300000</label>
                <br>
                <input type="checkbox" class="custom-control-input " id="customprice3" name="price4">
                <label class="custom-control-label" for="customCheck">300000-500000</label>
            </div>
        </form>
    </div>

    <hr style="border-top: 0.1rem solid #DEE2E6; ">


@model rlhTest.Models.DataModel.package_master

<div class="col-sm-4">
    <div class="col-item">
        <div class="photo">
            <img src="http://placehold.it/350x260" class="img-responsive" alt="a" />
        </div>
        <div class="info">
            <div class="row">
                <div class="price col-md-6">
                    <h5>
                       @Model.Package_Name
                    </h5>
                    <p>
                       @Model.Package_Description
                    </p>
                    <h5 class="price-text-color">
                        @Model.Package_Price
                    </h5>
                </div>
                <div class="rating hidden-sm col-md-6">
                </div>
            </div>
            <div class="clearfix">
            </div>
        </div>
    </div>
</div>

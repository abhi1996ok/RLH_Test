@model List<rlhTest.Models.DataModel.package_master>
<!-- Home page: Index view Element -->
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_home.cshtml";
}
<script>
    function dataStore()
    {
        var array = @Html.Raw(Json.Encode(
                   ((IEnumerable<rlhTest.Models.DataModel.package_master>)ViewBag.data).Select(dataS => new {
                   Booking_Enabled_Flag = dataS.Booking_Enabled_Flag,
                   Creation_Date = dataS.Creation_Date,
                   Customization_Allowed_Flag = dataS.Customization_Allowed_Flag,
                   C_PM_id = dataS.C_PM_id,
                   No_of_Days = dataS.No_of_Days,
                   Package_Description = dataS.Package_Description,
                   Package_Name = dataS.Package_Name,
                   Package_Price = dataS.Package_Price,
                   Booking_Count = dataS.Booking_Count


               })));
        

        window.localStorage.setItem("data", JSON.stringify(array));
        alert("Yes!" + array);
      

    }
    window.onload = dataStore();
</script>


<div class="container-fluid border" style="">
    <center>
        <h1>
            Hot Deals
        </h1>
